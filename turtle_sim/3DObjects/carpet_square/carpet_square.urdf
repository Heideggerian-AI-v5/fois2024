<?xml version="1.0"?>
<robot name="carpet_square">
  <link name="carpet">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0"/>
        <inertia ixx="4.4e-5" ixy="0" ixz="0" iyy="0" iyz="0" izz="4.4e-5"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="carpet.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="carpet.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.1"/>
      </surface>
    </collision>
  </link>
  <link name="deco">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0"/>
        <inertia ixx="4.4e-5" ixy="0" ixz="0" iyy="0" iyz="0" izz="4.4e-5"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="deco.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="deco.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.1"/>
      </surface>
    </collision>
  </link>
  <joint name="carpet_to_deco" type="fixed">
    <parent link="carpet"/>
    <child link="deco"/>
    <origin rpy="0 0 0" xyz="0 0 0.005"/>
  </joint>
</robot>

